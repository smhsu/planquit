{
  "name": "PlanQuit",
  "version": "1.0",
  "description": "Make a plan to help yourself stop wasting time on the Internet",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "permissions": [
      "tabs",
      "alarms",
      "idle"
  ],
  "web_accessible_resources": [
    "blockPage.html"
  ]
}
